{
  "name": "n8n-nodes-n8face",
  "version": "1.0.5",
  "description": "N8face API integration node for n8n",
  "keywords": [
    "n8n-community-node-package",
    "n8face"
  ],
  "license": "MIT",
  "homepage": "https://github.com/teamp8s/n8n-nodes-n8face?tab=readme-ov-file#n8face-node-for-n8n",
  "author": {
    "name": "Plus8Soft Team",
    "email": "it@plus8soft.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teamp8s/n8n-nodes-n8face.git"
  },
  "bugs": {
    "url": "https://github.com/teamp8s/n8n-nodes-n8face/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons && gulp build:post",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -- -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/N8faceApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/N8face/N8face.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
